{
  "name": "static-boilerplate",
  "version": "0.1.0",
  "description": "Simple boilerplate for static webpages.",
  "scripts": {
    "clean": "rimraf build",
    "start": "npm run clean && webpack-dev-server --no-info --inline --content-base build/ --port 3000",
    "build-dev": "npm run clean && cross-env NODE_ENV=development webpack",
    "build": "npm run clean && cross-env NODE_ENV=production webpack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/straku/static-boilerplate.git"
  },
  "keywords": [
    "boilerplate",
    "static webpage"
  ],
  "author": "Łukasz Strączyński <l.straczynski@hotmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/straku/static-boilerplate/issues"
  },
  "homepage": "https://github.com/straku/static-boilerplate#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^6.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.15.0",
    "image-webpack-loader": "^1.6.3",
    "node-sass": "^3.4.2",
    "postcss": "^5.0.19",
    "postcss-loader": "^0.8.2",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "normalize.css": "^4.0.0"
  }
}
